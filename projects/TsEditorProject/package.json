{
    "name": "tsproject",
    "version": "1.0.0",
    "description": "ts project assets",
    "scripts": {
        "tsc:watch": "tsc -watch -p tsconfig.json",
        "webpack:debug": "webpack --config webpack.config.js --watch",
        "webpack:publish": "webpack --config webpack.config.js",
        "xor:compile": "tsc -p tsconfig.json",
        "xor:compile-esm": "tsc -p tsconfig.json --module ES6",
        "xor:copy-js": "node ./xor-workflow.mjs copydir --filter=.js/.mjs --append=.txt --source=./output/xor --target=../Assets/XOR/Runtime/Resources/puerts/xor",
        "xor:copy-dts": "node ./xor-workflow.mjs copydir --filter=.ts --source=./output/xor --target=../Assets/XOR/Typing/puerts/xor",
        "xor:publish": "npm run xor:compile && npm run xor:copy-js && npm run xor:copy-dts",
        "xor:publish-esm": "npm run xor:compile-esm && npm run xor:copy-js && npm run xor:copy-dts && npm run xor:compile"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/node": "^18.7.23",
        "buffer": "^6.0.3",
        "eslint": "^7.32.0",
        "minimist": "^1.2.7",
        "source-map-support": "^0.5.19",
        "ts-loader": "^9.4.2",
        "typescript": "^4.6.3",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
    }
}